<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:VolunteerP.ViewModel"
                    xmlns:view="clr-namespace:VolunteerP.View">

    <DataTemplate DataType="{x:Type vm:PostVm}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UserPostsVm}">
        <view:UserPosts/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ProfileVm}">
        <view:Profile />
    </DataTemplate>


</ResourceDictionary>